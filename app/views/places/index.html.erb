<% if @current_user %>

<h1>Places</h1>

<p>
 <a class="btn btn-success" href="/places/new" role="button">New Place</a> 
</p>

<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place.id %>"><%= place.name %></a></li>
  <% end %>
</ul>

<% else %>

<h1> Please login or signup to see the Travel Log!  </h1>
<% end %>